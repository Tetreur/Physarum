{
  "version": "0.2",
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "words": [
    "animejs",
    "nextjs",
    "noopener",
    "noreferrer",
    "physarum",
    "turbopack",
    "vaul",
    "vercel",
    "webgpu",
    "wgsl"
  ],
  "enableFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "markdown",
    "mdx",
    "json"
  ],
  "files": [
    ".changeset/*.md",
    "**/*.md",
    "**/*.mdx",
    "**/*.json",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx"
  ],
  "useGitignore": true,
  "overrides": [],
  "ignorePaths": []
}
